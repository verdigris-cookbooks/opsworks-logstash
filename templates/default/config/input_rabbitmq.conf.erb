input {
  rabbitmq {
    host => <%= @rabbitmq_host %>
    port => <%= @rabbitmq_port %>
    <% if @rabbitmq_user %>
    user => <%= @rabbitmq_user %>
    <% end %>
    <% if @rabbitmq_password %>
    password => <%= @rabbitmq_password %>
    <% end %>
    exchange => <%= @rabbitmq_exchange %>
    exchange_type => <%= rabbitmq_exchange_type %>
  }
}
